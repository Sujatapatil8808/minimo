	display:inline-block;
	text-transform:uppercase;
}


  /* PHOTODIARY CSSS */

.thumbnailSection{
	padding:135px 0 125px;
}

.thumbnailSection2{
	padding:100px 0 65px;
}

.thumbImg{
	height:275px;
	margin-bottom:30px;
}

.thumbImg img{
	width:100%;
	height:100%;
	object-fit:cover;
}

.thumbnail .para{
	margin-bottom:0px;

}

.innerRow .col6:first-child{
	padding-left:0px;
}

.innerRow .col6:last-child{
	padding-right:0px;
}



 /* ##### SIGNUP CONTAINER #### */
 
 .signUp{
	 background-color:#f0f0f0;
	 padding:70px 0 75px 0;
 }
 
 .form-group{
	 position:relative;
 }
 
 .form-control{
	
	 display:block;
	 width:100%;
	 height:36px;
	 border:0px;
	 border-bottom:1px solid black;
	 background-color:transparent;
	 outline:none;
	 
 }
 
  .form-group i{
	  position:absolute;
	  top:10px;
	  right:10px;
	  transform:rotate(45deg);
	  font-size:26px;
	  cursor:pointer;
	  border:0px;
	  background-color:transparent;
  }
 
 .footer{
	 background-color:#626262;
	 padding: 40px 0;
 }
 
 .footerlink, .footerlink li{
	 display:inline-block;
 }
 
 .footerlink li{
	 color:#000000;
	 margin-left:15px;
	 
 }
 
 .footerlink li a{
	 color:solid black;
	 
 }